<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:BlinkBro"
             x:Class="BlinkBro.App">

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>
    
    <Application.Resources>
        <FontFamily x:Key="QuickSandRegular">avares://BlinkBro/Assets/Quicksand-Regular.ttf#Quicksand</FontFamily>
        <FontFamily x:Key="QuickSandBold">avares://BlinkBro/Assets/Quicksand-Bold.ttf#Quicksand</FontFamily>
    </Application.Resources>
</Application>